---
import BaseLayout from "../layouts/BaseLayout.astro";
const pageTitle = Astro.props;
import Greeting from "../components/Greeting";
---

<BaseLayout pageTitle={pageTitle}>
	<h2>My awesome blog subtitle</h2>
	<Greeting
		client:load
		messages={[
			"Hello World",
			"Hello Astro",
			"Hello React",
			"Hello Vue",
			"Hello Svelte",
			"Hello Preact",
			"Hello Solid",
			"Hello Lit",
		]}
	/>
</BaseLayout>
